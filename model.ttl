@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix schema: <http://schema.org/> .
@prefix my_ns: <http://dsci558.org/usbridges#> .


#### Bridge Class ####
my_ns:Bridge a schema:Class ;
    rdfs:subClassOf schema:Bridge ;
    schema:url schema:URL ;
    schema:name xsd:text ;
    schema:image schema:URL ;
    my_ns:place xsd:text ;
    my_ns:county xsd:text ;
    my_ns:state xsd:text ;
    my_ns:status xsd:text ;
    my_ns:year xsd:integer ;
    my_ns:rehabilitated xsd:boolean ;
    my_ns:builder xsd:text ;
    my_ns:traffic xsd:integer ;
    my_ns:lengthOfLargestSpan xsd:decimal ;
    my_ns:totalLength xsd:decimal ;
    my_ns:deckWidth xsd:decimal ;
    schema:latitude xsd:decimal ;
    schema:longitude xsd:decimal ;
    my_ns:overallCondition xsd:text ;
    my_ns:superstructureCondition xsd:text ;
    my_ns:substructureCondition xsd:text ;
    my_ns:deckCondition xsd:text ;
    my_ns:sufficiency xsd:decimal ;
    my_ns:route xsd:text ;
    my_ns:river xsd:text ;
    my_ns:open xsd:boolean ;
    my_ns:material xsd:text ;
    my_ns:design xsd:text ;
    my_ns:maintainer xsd:text .
